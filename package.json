{
	"name": "template-angular",
	"private": false,
	"version": "5.0.0",
	"author": "Gregory J Lopez",
	"homepage": "https://iamgld.com",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"start:open": "ng serve --open",
		"start:host": "ng serve --host 0.0.0.0",
		"start:dev": "ng serve --configuration development",
		"start:stag": "ng serve --configuration staging",
		"start:prod": "ng serve --configuration production",
		"build": "pnpm build:prod",
		"build:dev": "ng build --optimization --configuration development --progress",
		"build:stag": "ng build --optimization --configuration staging --progress",
		"build:prod": "ng build --optimization --configuration production --progress",
		"build:stats": "ng build --optimization --configuration development --stats-json",
		"test": "jest",
		"test:watch": "jest --watchAll",
		"prettier": "prettier --check src/app/**/*.ts",
		"prettier:fix": "prettier --write src/app/**/*.ts",
		"eslint": "eslint src/app/**/*.ts",
		"eslint:fix": "eslint --fix src/app/**/*.ts",
		"stylelint": "stylelint  src/**/*.{css,scss}",
		"stylelint:fix": "stylelint --fix  src/**/*.scss",
		"commitlint": "commitlint --edit $1",
		"analyze": "webpack-bundle-analyzer dist/stats.json",
		"postinstall": "husky install"
	},
	"dependencies": {
		"@angular/animations": "16.2.5",
		"@angular/cdk": "16.2.4",
		"@angular/common": "16.2.5",
		"@angular/compiler": "16.2.5",
		"@angular/core": "16.2.5",
		"@angular/forms": "16.2.5",
		"@angular/material": "16.2.4",
		"@angular/platform-browser": "16.2.5",
		"@angular/platform-browser-dynamic": "16.2.5",
		"@angular/router": "16.2.5",
		"@ngrx/effects": "16.2.0",
		"@ngrx/eslint-plugin": "16.2.0",
		"@ngrx/router-store": "16.2.0",
		"@ngrx/schematics": "16.2.0",
		"@ngrx/store": "16.2.0",
		"@ngrx/store-devtools": "16.2.0",
		"rxjs": "7.8.1",
		"tslib": "2.6.2",
		"zone.js": "0.13.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "16.2.2",
		"@angular-eslint/builder": "16.1.2",
		"@angular-eslint/eslint-plugin": "16.1.2",
		"@angular-eslint/eslint-plugin-template": "16.1.2",
		"@angular-eslint/schematics": "16.1.2",
		"@angular-eslint/template-parser": "16.1.2",
		"@angular/cli": "16.2.2",
		"@angular/compiler-cli": "16.2.5",
		"@commitlint/cli": "17.7.1",
		"@commitlint/config-conventional": "17.7.0",
		"@ngx-env/builder": "16.1.3",
		"@schematics/angular": "16.2.2",
		"@types/express": "4.17.17",
		"@types/jest": "29.5.4",
		"@types/marked": "4.3.0",
		"@types/node": "20.6.0",
		"@typescript-eslint/eslint-plugin": "6.7.0",
		"@typescript-eslint/parser": "6.7.0",
		"eslint": "8.49.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-prettier": "5.0.0",
		"husky": "8.0.3",
		"jest": "29.7.0",
		"jest-preset-angular": "13.1.1",
		"postcss-angular": "1.0.3",
		"prettier": "3.0.3",
		"stylelint": "15.10.3",
		"stylelint-config-standard": "34.0.0",
		"stylelint-config-standard-scss": "11.0.0",
		"stylelint-order": "6.0.3",
		"stylelint-selector-bem-pattern": "3.0.1",
		"typescript": "5.0.4",
		"webpack-bundle-analyzer": "4.9.1"
	},
	"jest": {
		"preset": "jest-preset-angular",
		"setupFilesAfterEnv": [
			"<rootDir>/config.jest.ts"
		],
		"moduleNameMapper": {
			"@app/(.)": "<rootDir>/src/app/$1",
			"@src/(.)": "<rootDir>/src/$1"
		}
	}
}
